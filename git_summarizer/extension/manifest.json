{
  "manifest_version": 3,
  "name": "GitHub Repo Analyzer",
  "version": "1.0",
  "description": "Analyzes GitHub repositories to provide detailed code flow descriptions",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://github.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 